{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "cmake config",
      "type": "shell",
      "command": "${workspaceFolder}/src/cmd_debug/config_cmake.cmd",
      "args": [],
      "problemMatcher": []
    },
    {
      "label": "cmake build",
      "type": "shell",
      "command": "${workspaceFolder}/src/cmd_debug/build_debug_venv.cmd",
      "args": [
        "${input:build_target}"
      ],
      "problemMatcher": []
    },
    {
      "label": "run_work_computer",
      "type": "shell",
      "command": "${workspaceFolder}/build_script/deploy/run_work_computer.cmd",
      "problemMatcher": []
    },
    {
      "label": "install",
      "type": "shell",
      "command": "${workspaceFolder}/build_script/build/install.cmd;${workspaceFolder}/build_script/build/updata_html.cmd",
      "problemMatcher": []
    },
    {
      "label": "build 5.2 plug and install",
      "type": "shell",
      "command": "${workspaceFolder}/script/uePlug/build_5.cmd;${workspaceFolder}/build_script/build/install.cmd;${workspaceFolder}/build_script/build/updata_html.cmd",
      "problemMatcher": []
    }
  ],
  "inputs": [
    {
      "id": "arg_input",
      "type": "promptString",
      "description": "input arg",
      "default": "dingding_base/client_get_user_attendance"
    },
    {
      "id": "build_target",
      "type": "pickString",
      "description": "要构建的目标",
      "options": [
        "DoodleExe",
        "doodle_core",
        "bvh_test",
        "dingding_base",
        "test_time"
      ]
    }
  ]
}