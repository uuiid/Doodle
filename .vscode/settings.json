    "qttools.injectNatvisFile": false,
    "testMate.cpp.log.logSentry": "disable_3",
    "cmake.configureSettings": {
        "gtest_build_samples": "ON",
    },
    "testMate.cpp.test.advancedExecutables": [
        {
            "name": "test_my",
            "pattern": "install/**/test_my.exe",
            "cwd": "${workspaceFolder}"
        }
    ],
    "C_Cpp.clang_format_style": "{BasedOnStyle: Google, SortIncludes: false, ColumnLimit: 0,AlignConsecutiveAssignments: true,AlignAfterOpenBracket: Align}",
    "cmake.buildDirectory": "${workspaceFolder}/b_${generator}_${buildKit}_${buildType}",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "python.envFile": "${workspaceFolder}/.env",
    "python.formatting.autopep8Args": [
        "--ignore",
        "E402"
    ],
    "editor.tabSize": 2,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 8,
    "editor.fontFamily": " 'Fira Code',Consolas, monospace",
    "editor.fontLigatures": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        ".idea": true,
        ".vs": true,
        "b_Ninja*": false,
        "install*": true
    },
    "editor.formatOnType": true
}